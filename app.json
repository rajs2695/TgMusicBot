    "COOKIES_URL": {
      "description": "Cookies URL for the bot; Note use BatBin urls",
      "value": "",
      "required": false
    },
    "DEFAULT_SERVICE": {
      "description": "Default search platform (Options: youtube, spotify, jiosaavn) Default: youtube",
      "value": "youtube",
      "required": false
