    "SUPPORT_GROUP": {
      "description": "Telegram Group Link",
      "value": "https://t.me/GuardxSupport",
      "required": false
    },
    "SUPPORT_CHANNEL": {
      "description": "Telegram Channel Link",
      "value": "https://t.me/FallenProjects",
      "required": false
    }
